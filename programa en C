#include<stdio.h>
int main(){
    float n=0;
    float b=0;
    float c=0;
    
	  printf("Dame un numero entero positivo:\n");
      scanf("%f",&n);
      printf("Dame un numero:\n");
      scanf("%f",&b);
      printf("Dame un numero:\n");
       scanf("%f",&c);
    
    printf("%f %f %f estos son tus numeros", n,b,c);
    
 
if( (n>0&&b>0&&c>0)&&(n!=b!=c)  )  {
	
		{//inicio del else
           if(n>b>c)
		printf("%d es el mayor",n);
        }else{
        printf("%d es el mayor",b);        
        }else{
        printf("%d es el mayor",c);        
        }else//Fin del else
           system("PAUSE");
    } 
 if( (n<0&&b<0&&c<0)&&(n!=b!=c)  )  {
	
		{//inicio del else
           if(n<b<c)
		printf("%d es el mayor",n);
        }else{
        printf("%d es el mayor",b);        
        }else{
        printf("%d es el mayor",c);        
        }else{
		//Fin del else
           system("PAUSE");
    } 
	printf("Fin de programa");
}
